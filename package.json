  "scripts": {
    "build": "npm run clean && rollup -c",
    "clean": "rimraf dist",
    "lint": "eslint --color -c .eslintrc.js src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore",
    "prettier:format": "npm run prettier -- --write \"**/*.+(ts|tsx)\"",
    "prettier:format:staged": "npm run prettier -- --write",
    "test": "jest",
    "test:watch": "jest --watch  --coverage=false",
    "coverage": "jest --watchAll=false --coverage",
    "coverage-diff": "jest --watchAll=false --coverage --changedSince origin/main",
    "start": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "rimraf storybook-static && storybook build",
    "push": "npm run build && yalc push"
  },
